<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <template id="patient_card">
         <t t-call="web.external_layout">
         <t t-call="web.html_container">
              <t t-foreach="docs" t-as="doc">
                  <h2>Patient Card</h2>
                  Age: <t t-esc="doc.patient_age"/>
                  Patient Name: <t t-esc="doc.patient_name"/>
              </t>
         </t>
         </t>
     </template>


</odoo>
